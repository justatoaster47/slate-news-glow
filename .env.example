# Example environment variables - Copy to .env.local and fill in values
# DO NOT COMMIT .env.local

# Supabase (Get from your Supabase project settings)
NEXT_PUBLIC_SUPABASE_URL=YOUR_SUPABASE_URL
NEXT_PUBLIC_SUPABASE_ANON_KEY=YOUR_SUPABASE_ANON_KEY
NEXT_PUBLIC_SITE_URL=http://localhost:3000 # Or your deployment URL

# External APIs (Obtain keys from respective services)
NEWS_API_KEY=
ALPHA_VANTAGE_API_KEY=
FMP_API_KEY=
FRED_API_KEY=
BLS_API_KEY=
SEC_API_USER_AGENT= # Format: YourName YourEmail@example.com
BEA_API_KEY=
EIA_API_KEY=
CONGRESS_API_KEY=
USASPENDING_API_KEY=
FEDERAL_REGISTER_API_KEY= # Usually doesn't require a key but check docs
CENSUS_API_KEY=

# keys to call supabase functions / access api from client.
ADMIN_SECRET_TOKEN= # Secret token for admin access. generate youreself.
SUPABASE_SERVICE_ROLE_KEY= # Service role key for supabase. find in project settings -> data api

# AI Provider (Choose one or more)
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
GEMINI_API_KEY= 
